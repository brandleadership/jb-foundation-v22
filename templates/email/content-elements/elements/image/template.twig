        <tbody>
            <tr>
                <td width="{{ _imageWidth ? : '530' }}" style="padding-top: 35px;">
                    <img style="border-radius: 5px;" data-bsi-element-part="image" width="{{ _imageWidth ? : '530' }}" alt="" src="{{ _imageSrc ? : 'img/content.png' }}" />
                </td>
            </tr>
        </tbody>
    </table>
</div>
